&lt;script type="text/javascript"&gt;
(function($){
	var cookieDomain = window.location.hostname.replace(/[-\w]+(\.\w[-\w]*)+(\.\w[-\w]*)+/, "$1$2");
	if(document.cookie.indexOf('showBrowserUpgradeMsg=1') != -1){
		$('.message-upgrade-browser').hide();
	}
	
	//If any page popup links are clicked, set the cookie to never show it again
	$('.message-close').click(function(){
		document.cookie="showBrowserUpgradeMsg=1; path=/; domain=" + cookieDomain;
		$('.message-upgrade-browser').hide();
	});
})(cyljq);
&lt;/script&gt;
