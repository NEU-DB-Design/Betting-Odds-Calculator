  <script>
   cyljq(function(jq){
    (function(selectors){
    	var VCLICK_EVENT_NAME = 'vclick',
    		eventRanOnce = false;
    		jqDoc = jq(document);
    		
    	jqDoc.on('touchstart.trackFakeLink', function(ev){
    		var el = ev.target;
    		
    		jqDoc.one('touchmove.' + VCLICK_EVENT_NAME + ' touchstart.' + VCLICK_EVENT_NAME + ' touchend.' + VCLICK_EVENT_NAME, function(te_ev){
    			jqDoc.off('.' + VCLICK_EVENT_NAME);
    			if(ev.target === te_ev.target &amp;&amp; te_ev.type !== 'touchmove'){
    				var newEvent = jq.Event(VCLICK_EVENT_NAME, jq.extend({}, te_ev, {type:VCLICK_EVENT_NAME}));
    				jqDoc.trigger(newEvent)
    			}
    		});
    	});
    	
    	jqDoc.on('click.trackFakeLink vclick.trackFakeLink', selectors.join(','), function(ev){
    		if(eventRanOnce == false) {
    			if(ev.type === VCLICK_EVENT_NAME){
    				jqDoc.off('click.trackFakeLink');
    			}else{
    				jqDoc.off('vclick.trackFakeLink touchstart.trackFakeLink');
    			}
    			
    			eventRanOnce = true;
    		}
    		if(typeof fed !== 'undefined' &amp;&amp; fed.analytics){
    			fed.analytics.trackEvent({event: 'click:Heading', category: 'Heading', label: jq.trim(jq(ev.target).text()), nonInteraction: false});
    		}
    	});
    })(
    /* List of selectors that look like links */
    [
    '.heading-1',
    '.heading-2', 
    '.heading-2-normal',
    '.heading-7',
    '.alt-lg-titled-list-header',
    '.standard-titled-list-header',
    '.box-info-hdr-underlined.txt-h2-hi',
    '.txt-h2',
    '.main-page-title',
    '.text-h-bordered',
    '.paint-transparent-eyeshadow.text-h5.text-underline'
    ]);
});
  </script>
