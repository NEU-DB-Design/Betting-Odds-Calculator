   <!-- HAU test : promo code response. Again, doing inline styles so I don't corrupt our sheets. This is obviously dirty code as it's going to be ripped out. -->
   <div id="vdcPromoCodePanel" class="promo-code-widget-container" data-fed-plugin="widget" data-fed-widget-url="/mytrip/app/Widget?widgetCode=HeardAboutUs" data-fed-widget-type="replace" data-fed-widget-update-content-selector="#promoCodeWidget" data-fed-widget-update-url="/mytrip/app/PromoCodeWidgetServicePage">
   </div>
   <script>
    cyljq(function heardAboutUs(jq){
	//this is just to dirty test promo code stuff
	jq( "#vdcPromoCodeForm" ).submit(function( event ) {
		var enteredCode = jq("input#promoCode111").val();
		var label = 'Submit code: ' + enteredCode;
		
		fed.analytics.trackEvent({ event: 'click:Button', category: 'SiteHeader', label: label, nonInteraction: false });
		jq(document).trigger({ type:"widgetSubmitted", params: enteredCode });
		jq(this).closest('.popover').fed('popover', 'toggle');
		
		
		//Prevent the form from being submitted
		event.preventDefault();
	});
	
	//Fire impressions for the feedback panel 
	jq('#vdcPromoCodePanel').on('widgetReplaced', function(ev, content){
		var label = cyljq('#cyl-promo-code-feedback').text();
		
		if(label){
			fed.analytics.trackEvent({
				event : 'impression:Error',
				nonInteraction : true,
				label : label,
				category : 'SiteHeader'
			});
		}
	});
});
   </script>
   <!-- end test -->
